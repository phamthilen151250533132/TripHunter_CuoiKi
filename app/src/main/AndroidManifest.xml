<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chinhpc.triphunter">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logotripp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activity_Layout.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Layout.trangchinh.Main_2Activity"
            android:label="@string/title_activity_test"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".Activity_Layout.login_register.RegisterActivity" />
        <activity android:name=".Activity_Layout.login_register.LoginActivity" />
        <activity android:name=".Activity_Layout.trangchinh.TrangChinhActivity" />
        <activity android:name=".Activity_Layout.canhan.ProfileActivity" />
        <activity android:name=".Activity_Layout.trangchinh.NoiDung_diadanh" />
        <!-- suppress AndroidDomInspection -->
        <activity android:name=".Activity_Layout.trangchinh.DiaDanhActivity" />
        <activity android:name=".Activity_Layout.trangchinh.TrangChinh" />
        <activity android:name=".Activity_Layout.trangchinh.ChiTietDiaDanh" />
        <activity android:name=".Activity_Layout.login_register.login_trangchinh" />
        <activity android:name=".Activity_Layout.canhan.CaNhanActivity"></activity>
    </application>

</manifest>